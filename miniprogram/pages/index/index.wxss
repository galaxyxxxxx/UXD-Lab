page{
  font-size: var(--font-size-S);
  height: 100%;
}

.container{
  height: calc(100vh);
  /* background :linear-gradient(#3C77FF,#00C2FF,#00C2FF, #00C2FF); */
  background: url(http://60.205.155.131/wp-content/uploads/2021/03/bg2.jpg);
  background-size: 100% 100%;
  /* background-position-y: -1px; */
}


/* 授权页 */
.authrization{

  font-family: Verdana;
  width: 100%;
  height: 100vh;

  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;

  background-color: rgb(253, 252, 252);

}
.authMain{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  border-radius: 25rpx;
  padding: 30rpx;
  position: relative;
  top: -100rpx;
}
.authMain image{
  width: 200rpx;
  height: 200rpx;
  border-radius: 50%;
}
.content{
  margin: 50rpx 0;
  line-height: 50rpx;
  text-align: center;
  color: rgb(44, 44, 44);
}
.id{
  font-size: 35rpx;
  font-weight: bold;
  margin-bottom: 50rpx;
  background: linear-gradient(45deg,#3C77FF,#00C2FF);
  -webkit-background-clip: text;
  color: transparent;
}
.description{
  font-size: 50rpx;
  font-weight: bold;
  margin: 50rpx 0;
  line-height: 2rem;
}
.applyTitle{
  color: #999;
  font-size: 20rpx;
  line-height: 1rem;
  margin: 0 60rpx;
}
.loginBtn{
  margin-top: 100rpx;
  width: calc(80vw);
  border-radius: 50rpx;
  background: linear-gradient(45deg,#3C77FF,#00C2FF);
  color: #fff;
  letter-spacing: 6rpx;
  font-weight: bold;
  box-shadow: 0 10px 20px #fff;
}

/* 顶部个人信息 高10%*/

.userinfo{
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #f0f0f1;
  border-radius: 8rpx;
  width: calc(94vw);
  height: calc(10vh);
  /* background-color: rgba(236, 236, 236, 0.185); */
}
.hello{
  line-height: 50rpx;
  font-size: 28rpx;
}
.today{
  font-size: 52rpx;
  line-height: 70rpx;
  /* color: rgb(99, 99, 99); */
  font-weight: bold;
}
.avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 20rpx 0;
  border: 1px solid rgb(99, 99, 99, 0.1);
}
.nickname{
  margin-left: 20rpx;
}

/* 主体 */
.main {
  width: 100%;
  /* overflow: scroll; */
  top:  calc(15vh);
  height: calc(85vh);
  
  /* display: flex;
  flex-direction: row;
  justify-content: left; */
  /* background : rgba(224, 212, 212, 0.404); */
}

/* 时间线 */
.timeline{
  margin-top: calc(5vh);
  width: calc(10vw);
  height: calc(85vh);

  float: left;
  display: flex;
  flex-direction: column;
  align-content: center;

  background-color: #f9f9fa;
  color: rgb(99, 99, 99);
}
.hourpoint{
  height: calc(8vh);
  display: flex;
  justify-content: flex-end;
  align-items: center;
  opacity: 0.6;
}

/* 滚动区域 width90%*/
.scroll{
  /* float: auto; */
  display: flex;
  height: calc(90vh);
  width: calc(90vw);
  overflow: hidden;
}
/* 一列 */
.day{
  /* flex: 1; */
  min-width: calc(30vw);
  height: calc(90vh);
  display: flex;
  flex-direction: column;
  background: url('data:image/svg+xml;base64,');
  background-position-y: calc(5vh);
  background-position-x: -5px;
  background-size: auto calc(80vh);
  background-repeat: no-repeat;
  /* border-left: 1rpx solid #0f0f13; */
}
/* 日期 */
.date{
  height:calc(4vh);
  text-align: center;  
  font-size: 28rpx;
  color: #fff;
  line-height: calc(4vh);
  
}

