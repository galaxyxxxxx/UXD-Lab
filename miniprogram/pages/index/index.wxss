page{
  font-size: var(--font-size-S);
  font-family: sans-serif;

  background :linear-gradient(135deg,#3C77FF,#00C2FF,#00C2FF, #00C2FF)
}

.container{
  height: 110vh;
}


/* 授权页 */
.authrization{
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}

.content{
  margin: 50rpx 0;
  line-height: 50rpx;
}

.loginBtn{
  background: linear-gradient(right, #fff, #fff999);
}

/* 顶部个人信息 高120rpx*/

.infoOut{
  width: 94%;
  margin: 180rpx 0rpx 0rpx 0rpx;
  padding: 20rpx 0 0;
  border-radius: 10rpx;
  position: fixed;
  /* background :linear-gradient(135deg,#3C77FF, #00C2FF); */
}

.userinfo{
  margin: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #f0f0f1;
  /* border-bottom: 1rpx solid seagreen; */
  /* padding: 20rpx  18rpx; */
  border-radius: 8rpx;
  /* background-color: rgba(0, 255, 255, 0.11); */
}

.hello{
  /* color: rgb(99, 99, 99, .8); */
  
  /* font-weight: bold; */
  line-height: 50rpx;
  font-size: 28rpx;
}
.today{
  font-size: 52rpx;
  line-height: 70rpx;
  /* color: rgb(99, 99, 99); */
  font-weight: bold;
}
.avatar{
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  overflow: hidden;
  margin: 20rpx 0;
  border: 1px solid rgb(99, 99, 99, 0.1);
}
.nickname{
  margin-left: 20rpx;
}
.month {
  margin-left: 10rpx;
  font-size: var(--font-size-L);
  /* color: #ffffff; */
}

/* 主体 */
.main {
  width: 100%;
  position: fixed;
  top: 340rpx;
  /* height: 100%; */
  display: flex;
  flex-direction: row;
  justify-content: left;
}

/* 时间线 */
.timeline{
  margin-top: 75rpx;
  width: 80rpx;
  padding: 60rpx 10rpx 10rpx 10rpx;
  display: flex;
  flex-direction: column;
  align-content: center;
  background-color: #f9f9fa;
  color: rgb(99, 99, 99);
}
.hourpoint{
  margin: 10rpx 0 80rpx 0;
  /* width: 90rpx; */
  text-align: center;
  opacity: 0.6;
}
/* 滚动区域 width320px*/
.scroll{
  /* height: 100%; */
  width: 654rpx;
  /* background-color: #f9f9fa; */
  display: flex;
  overflow: scroll;
  
  /* border: 1rpx solid #000 */
}
/* 一列 */
.day{
  /* flex: 1; */
  min-width: 250rpx;
  /* height: 100%; */
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  background: url('data:image/svg+xml;base64,');
  /* background-size: auto 252rpx; */
  background-position-y: 76rpx;
  /* background-position-x: -30rpx; */
  background-repeat: no-repeat;
  /* background-color: #9696f0b0; */
  /* border-left: 1rpx solid rgba(0, 0, 0, 0.24); */
}
/* 日期 */
.date{
  height:80rpx;
  text-align: center;  
  font-size: 28rpx;
  /* color: rgb(99, 99, 99); */
  color: #fff;
  line-height: 80rpx;
  /* color: #fff; */
  /* background-color: #fff; */
  font-weight: bold;
}


/* 活动 */
.lab{
  display: block;
  flex-direction: column;
  width: 200rpx;
  position: absolute;
  padding: 0 18rpx;
  margin: 0 4rpx;
  border: 2rpx solid #3d77ff34;
  background-color: var(--color-theme2);
  border-radius: 8rpx;
  color: var(--color-theme);
  overflow: visible;
  /* letter-spacing: 1px; */
  line-height: 50rpx;
}

.lab:active{
  opacity: 0.6;
  transform: scale(1.02);
  border: 2rpx solid #3D78FF;
}

.titleMini{
  /* margin-top: 10rpx; */
  font-weight: bold;
  line-height: 30rpx;
  font-size: 28rpx;
}

.title{
  margin-top: 16rpx;
  font-weight: bold;
  line-height: 30rpx;
  font-size: 28rpx;
}

.time{
  font-size: var(--font-size-S);
  /* opacity: 0.5; */
}

.duration{

}

.add{
  z-index: 1;
  position: fixed;
  right: 50rpx;
  bottom: 80rpx;
  width: 100rpx;
  height: 100rpx;
  line-height: 100rpx;
  border-radius: 50%;
  box-shadow: -4px -4px 10px -8px rgba(255, 255, 255, 1), 4px 4px 10px -8px rgba(0, 0, 0, 0.5);
  background-color: var(--color-theme);
  text-align: center;
  vertical-align: center;
  color: #ffffff;
  font-size: var(--font-size-XL);
}

.history{
  width: 100rpx;
  text-align: center;
  position: fixed;
  left: 0;
  top: 350rpx;
  color: #fff;
  /* background-color: green; */
  z-index: 2;
}