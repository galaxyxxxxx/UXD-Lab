/* 活动 */
.lab{
  position: absolute;
  display: flex;
  flex-direction: column;
  justify-content: center;

  width: calc(30vw - 50rpx);
  margin-left: 16rpx;
  padding: 0 18rpx;

  border: 2rpx solid #3d77ff34;
  background-color: var(--color-theme2);
  border-radius: 8rpx;

  color: var(--color-theme);
  line-height: 50rpx;
}

.lab:active{
  opacity: 0.6;
  transform: scale(1.02);
  border: 2rpx solid #3D78FF;
}

.titleMini{
  /* margin-top: 10rpx; */
  font-weight: bold;
  line-height: 30rpx;
  font-size: 24rpx;
}

.title{
  margin-top: 16rpx;
  font-weight: bold;
  line-height: 30rpx;
  font-size: 28rpx;
}

.time{
  font-size: var(--font-size-S);
}

.add{
  width: 100rpx;
  height: 100rpx;

  z-index: 1;
  position: fixed;
  right: 50rpx;
  bottom: 80rpx;

  background: url(data:image/svg+xml;base64,);
  background-size: 40% 40%;
  background-repeat: no-repeat;
  background-position: center;
  /* background-color: #3C77FF; */
  background-color: #FFAD3E;
  border-radius: 50%;
  /* box-shadow: 0 10rpx 15rpx #3c76ff7e; */
  box-shadow: 0 10rpx 15rpx #FFAD3E7e;
}


.history{
  width: 100rpx;
  text-align: center;
  position: fixed;
  left: 0;
  top: 350rpx;
  color: #fff;
  /* background-color: green; */
  z-index: 2;
}

.order{
  width: calc(10vw);
  height: calc(5vh);

  z-index: 1;
  position: absolute;
  left: calc(1vw);
  top: calc(9.5vh);

  background: url(data:image/svg+xml;base64,);
  background-size: 70% 70%;
  background-repeat: no-repeat;
  background-position: center;
  /* background-color: #3cff668c; */
  /* background-color: #FFAD3E; */
}